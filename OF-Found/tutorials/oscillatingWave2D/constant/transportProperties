FoamFile
{
    format      ascii;
    class       dictionary;
    location    "constant";
    object      transportProperties;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

phases (water air);

water
{
    transportModel  Newtonian;
    nu              1.0e-6;
    rho             998.2;
}

air
{
    transportModel  Newtonian;
    nu              1.53e-5;
    rho             1.19;
}

sigma           72.74e-3;


